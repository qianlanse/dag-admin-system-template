{
    "$schema": "https://unpkg.com/@changesets/config@3.0.3/schema.json",
    "changelog": [
        "@changesets/changelog-github",
        { "repo": "qianlanse/dag-admin-system-template" }
    ],
    "commit": false,
    "fixed": [["@dag-core/*", "@dag/*"]],
    "snapshot": {
        "prereleaseTemplate": "{tag}-{datetime}"
    },
    "privatePackages": { "version": true, "tag": true },
