{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Web Package",
    "extends": "./base.json",
    "compilerOptions": {
        "jsx": "preserve",
        "jsxImportSource": "vue",
        "lib": ["ESNext", "DOM", "DOM.Iterable"],
        "useDefineForClassFields": true,
        "moduleResolution": "Bundler",
        "types": ["vite/client"],
        "declaration": false
    }
}
