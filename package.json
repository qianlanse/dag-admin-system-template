{
    "name": "small-vite-next-template",
    "version": "1.0.0",
    "private": true,
    "license": "MIT",
    "type": "module",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "postinstall": "turbo run stub",
        "commit": "czg",
        "prepare": "husky",
        "clean": "vsh clean",
        "format": "vsh lint --format",
        "lint": "vsh lint",
        "reinstall": "pnpm clean --del-lock && pnpm install",
        "check:cspell": "cspell lint **/*.ts **/README.md .changeset/*.md --no-progress"
    },
    "devDependencies": {
        "@dag/commitlint-config": "workspace:*",
        "@dag/eslint-config": "workspace:*",
        "@dag/prettier-config": "workspace:*",
        "@dag/stylelint-config": "workspace:*",
        "@dag/tsconfig": "workspace:*",
        "@dag/vsh": "workspace:*",
        "@types/node": "^22.5.5",
        "cspell": "^8.14.4",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "turbo": "^2.1.1",
        "typescript": "catalog:",
        "unbuild": "^2.0.0",
        "vite": "^5.4.3",
        "vue": "^3.5.3"
    },
    "engines": {
        "node": ">=20.10.0",
        "pnpm": ">=9.5.0"
    },
    "packageManager": "pnpm@9.11.0",
    "pnpm": {
        "peerDependencyRules": {
            "allowedVersions": {
                "eslint": "*"
            }
        }
    }
}
